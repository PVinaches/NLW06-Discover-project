<!DOCTYPE html>
    <head>        
        <title>Dato incorreta</title>
        <script>
            

            // Redireccionar a p√°gina original
            <% if(option === "1"){ %>
                // Alerta de sala incorreta
                confirm("Sala incorreta")
                
                window.location.replace("/")

            <%}else if(option === "2") { %>
                // Alerta de senha incorreta
                confirm("Senha incorreta")
                
                window.location.replace("/room/<%= roomId %>")

            <%}else if(option === "3") { %>
                // Alerta de senha incorreta
                confirm("Precisa uma senha")
                
                window.location.replace("/create-pass")
            <% } %>
            
        </script>
    </head>
</html>